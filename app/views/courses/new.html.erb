<%= form_for @course do |f| %>
    <%= f.label :name, "Name"%>
    <%= f.text_field :name%>
    <%= f.label :seat_limit, "Seat Limit"%>
    <%= f.number_field :seat_limit%>
    <%= f.label :classroom_number, "Classroom Number"%>
    <%= f.number_field :classroom_number%>
    <%= f.submit 'Submit New Course'%>
<%end%>